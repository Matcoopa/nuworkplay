<!doctype html>
<html>
<head>
  <meta content="IE=edge" http-equiv="X-UA-Compatible">
  <meta charset="utf-8">
  <meta content="width=device-width,initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no" name="viewport">
  <!-- Use title if it's in the page YAML frontmatter -->
  <title><%= current_page.data.title || "Nuworkplay" %></title>
  <%= stylesheet_link_tag :site %>
  <link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/jquery.slick/1.6.0/slick.css"/>
  <link href="https://fonts.googleapis.com/css?family=BioRhyme:300|Space+Mono" rel="stylesheet">
  <script src="https://use.fontawesome.com/6b450b718f.js"></script>
</head>
<body>
  <%= yield %>
  <div class="landing-footer">
    <%= partial "footer" %>
  </div>
  <script type="text/javascript" src="//code.jquery.com/jquery-1.11.0.min.js"></script>
  <script type="text/javascript" src="//code.jquery.com/jquery-migrate-1.2.1.min.js"></script>
  <%= javascript_include_tag :all %>
  <%= javascript_include_tag :segment_tracking %>
  <script type="text/javascript" src="//cdn.jsdelivr.net/jquery.slick/1.6.0/slick.min.js"></script>
</body>
</html>
